      "item": [
        {
          "name": "Login - Get Token",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var token = pm.response.json().data.token;",
                  "    pm.environment.set('token', token);",
                  "    pm.test('Token saved successfully', function() {",
                  "        pm.expect(token).to.exist;",
                  "    });",
                  "}",
                  "",
                  "pm.test('Login response structure', function() {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.status).to.equal('OK');",
                  "    pm.expect(jsonData.data.token).to.exist;",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"username\": \"admin\",\n    \"password\": \"1\"\n}"
            },
            "url": {
              "raw": "http://localhost:5000/api/auth/login",
              "protocol": "http",
              "host": ["localhost"],
              "port": "5000",
              "path": ["api", "auth", "login"]
