        {
          "name": "Create Simple Order (1 item)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Order created successfully', function() {",
                  "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
                  "});",
                  "",
                  "pm.test('Response has order data', function() {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.data.id).to.exist;",
                  "    pm.expect(jsonData.data.order_code).to.match(/^ORD-\\d{6}-\\d{5}$/);",
                  "    pm.expect(jsonData.data.status).to.equal('pending');",
                  "});",
                  "",
                  "pm.test('Order code format is correct', function() {",
                  "    var jsonData = pm.response.json();",
                  "    var code = jsonData.data.order_code;",
                  "    pm.expect(code).to.include('ORD-');",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
